	<meta content="{{p_description}}" name="description">
	<meta content="{{p_description}}" property="og:description">
	<meta content="/uploads/{{p_picture}}" property="og:image">
	<link href="/img/favicon.png" rel="icon">
	<meta property="fb:app_id" content=""/>
