	<meta name="twitter:creator" content="Emekatwobrothers.com"/>
	<meta name="twitter:image" content="/uploads/{{p_picture}}"/>
	<script src="/js/config.js"></script>
	<script src="/frameworks/jquery/jquery-3.4.1.min.js"></script>
	<script src="/frameworks/popper/popper.min.js"></script>
	<link rel="stylesheet" href="/frameworks/bootstrap/css/bootstrap.min.css">
	<script src="/frameworks/bootstrap/js/bootstrap.min.js"></script>
	<link href="/frameworks/font-awesome/css/font-awesome.min.css" rel="stylesheet">
	<script src="/frameworks/bootbox.min.js"></script>
	<link href="/frameworks/style.css" rel="stylesheet">
	<script src="/frameworks/index.js"></script>
